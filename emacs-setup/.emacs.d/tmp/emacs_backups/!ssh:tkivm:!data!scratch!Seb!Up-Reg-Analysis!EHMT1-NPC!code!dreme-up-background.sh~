#!/bin/sh

# Unlock the file to wortk with it 
datalad unlock Upregulated_500.fa.out
datalad unlock Downregulated_500.fa.out

# Rund Dreme
singularity/meme.simg dreme -verbosity 1 \
                      -oc output/dreme-out/up_down \
                      -dna \
                      -p data/fasta/Upregulated_500.fa.out \
                      -n data/fasta/Downregulated_500.fa.out \
                      -t 18000 \
                      -e 0.05 \
                      -maxk 20
